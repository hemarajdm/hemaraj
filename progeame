#include<stdio.h>
int main()
int age,usn;
char name,clg;
float weight,height;
printf("STUDENT DETAILS\n");
printf("enter the name:");
scanf("%s",&name);
printf("enter your age:");
scanf("%d",&age);
printf("enter your weight:");
scanf("%f",&weight);
printf("enter your height:");
scanf("%f",&height);
printf("enter usn:");
scanf("%d",&usn);
printf("enter your clg name:");
scanf("%s",&clg);
return 0;
