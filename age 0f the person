#include<stdio.h>
int main()
{
    int age;
    printf("enter the age:");
    scanf("%f",&age);
    if(age>=18)
    {
        printf("the person is adult");
    }
    else if(age>18)
    {
        printf("the person is not adult");
        
    }
    else if(age>=60)
    {
        printf("the person is senior citizen");
    }
}
